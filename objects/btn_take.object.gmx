            <kind>1</kind>
            <string>//check if click is in the button
a = collision_point (mouse_x, mouse_y, all, true, false)
//if click is in the button
if a != noone &amp;&amp; a.button = 1
{
    if a = id
    {
        //set the item of the inventory in the empty place
        with (obj_inventory)
        {
        incoming = btn_take.father
            for (i = 0; i &lt; 5; i += 1)
            {
                //if there's no empty slot, inventory full
                if i == 4
                {
                scr_info("I can't carry more objects.")
                obj_girl.action = 0
                }
                else
                {
                    //if there's a free slot
                    if slot[i] == 0
                    {
                        if incoming.y &gt; obj_girl.y + 60
                            obj_girl.action = 6
                        else
                            obj_girl.action = 7
                        obj_girl.image_index = 0
                        //set the the item (object is the type of item, and is given by the father of the button)
                        slot[i] = btn_take.object
                        scr_inventoryadd(btn_take.object,i,btn_take.state)
                        obj_inventory.active = i
                        with (incoming) {instance_destroy()}
                        break;
                    }
                }
            }
        }
        //kill buttons
        scr_kill_buttons()
    }
}
//if click is outside the button kill buttons and restore girl normal state
else
{
    scr_kill_buttons()
    obj_girl.action = 0
}
</string>
